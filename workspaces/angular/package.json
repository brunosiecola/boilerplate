{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "boilerplate-user:start:local": "ng serve boilerplate-user --port=5011",
    "boilerplate-administrator:start:local": "ng serve boilerplate-administrator --port=6011",
    "boilerplate:start:local": "concurrently --kill-others \"npm run boilerplate-user:start:local\" \"npm run boilerplate-administrator:start:local\"",
    "dev:ssr": "ng run boilerplate-administrator:serve-ssr",
    "serve:ssr": "node dist/boilerplate-administrator/server/main.js",
    "build:ssr": "ng build && ng run boilerplate-administrator:server",
    "prerender": "ng run boilerplate-administrator:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.7",
    "@angular/common": "^18.2.7",
    "@angular/compiler": "^18.2.7",
    "@angular/core": "^18.2.7",
    "@angular/forms": "^18.2.7",
    "@angular/platform-browser": "^18.2.7",
    "@angular/platform-browser-dynamic": "^18.2.7",
    "@angular/platform-server": "^18.2.7",
    "@angular/router": "^18.2.7",
    "@angular/ssr": "^18.2.7",
    "express": "^4.15.2",
    "gsap": "^3.12.2",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "~18.2.7",
    "@angular/compiler-cli": "^18.2.7",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^14.15.0",
    "concurrently": "^8.0.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^18.2.1",
    "typescript": "~5.5.4"
  }
}
